{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "out",
		"lib": [
			"ES2020"
		],
		"skipLibCheck": true,
		"sourceMap": true,
		"rootDir": "ext-src",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"useUnknownInCatchVariables": true
	},
	"include": [
		"ext-src"
	]
}
